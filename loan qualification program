// program for qualification of a loan
#include <stdio.h>

int main() {
    int age;
    int income;
    printf("please dial in your age:");
    scanf("%d",&age);
    printf("pleae dial in your income:");
    scanf("%d",&income);
    if (age>=21&&income>=21000){
    printf("conglatulations you qualify for a loan.\n");
    }else{
    printf("unfortunately we are unable to offer you a loan at this time.\n");}
    
    return 0;
}
